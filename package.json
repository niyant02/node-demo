{
    "name": "demo",
    "version": "1.0.0",
    "description": "nodejs, postgresql, graphql",
    "main": "index.js",
    "scripts": {
        "start": "npm run build && node dist/index.js",
        "dev": "ts-node-dev --no-notify --respawn --transpile-only src/index.ts",
        "build": "rimraf ./dist && tsc && cp .env dist/ && cd dist && mkdir uploads && cd uploads && mkdir images",
        "dev:typecheck": "tsc --noEmit --watch",
        "generate:all": "yarn migrate && yarn generate:prisma && yarn generate:nexus && yarn seed:prisma",
        "generate:nexus": "ts-node --transpile-only src/schema",
        "generate:prisma": "prisma generate",
        "seed:prisma": "prisma db seed --preview-feature",
        "migrate": "prisma migrate dev --name init --preview-feature",
        "migrate:reset": "prisma migrate reset --preview-feature",
        "migrate:deploy": "prisma migrate deploy --preview-feature",
        "migrate:update": "prisma migrate dev --create-only --preview-feature",
        "lint": "eslint '*/**/*.{js,ts,tsx}' --quiet --fix"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/niyant02/node-demo.git"
    },
    "author": "niyant02",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/niyant02/node-demo/issues"
    },
    "homepage": "https://github.com/niyant02/node-demo#readme",
    "devDependencies": {
        "@types/bcryptjs": "^2.4.2",
        "@types/body-parser": "^1.19.0",
        "@types/cors": "^2.8.10",
        "@types/express": "^4.17.11",
        "@types/graphql-iso-date": "^3.4.0",
        "@types/graphql-upload": "^8.0.4",
        "@types/jsonwebtoken": "^8.5.1",
        "@types/lodash": "^4.14.168",
        "@types/node": "^15.6.2",
        "@types/slug": "^0.9.1",
        "@typescript-eslint/eslint-plugin": "^4.17.0",
        "@typescript-eslint/parser": "^4.17.0",
        "eslint": "^7.21.0",
        "eslint-config-prettier": "^8.1.0",
        "eslint-config-standard": "^16.0.2",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-prettier": "^3.3.1",
        "eslint-plugin-promise": "^4.3.1",
        "faker": "^5.4.0",
        "prettier": "^2.2.1",
        "prisma": "2.18.x",
        "rimraf": "^3.0.2",
        "ts-node": "^10.0.0",
        "ts-node-dev": "^1.1.6",
        "typescript": "^4.3.2"
    },
    "dependencies": {
        "@prisma/client": "2.18.x",
        "apollo-server": "^2.21.1",
        "apollo-server-errors": "^2.4.2",
        "bcryptjs": "^2.4.3",
        "blob": "^0.1.0",
        "body-parser": "^1.19.0",
        "cors": "^2.8.5",
        "dotenv": "^10.0.0",
        "express": "^4.17.1",
        "express-formidable": "^1.2.0",
        "floggy": "^0.2.1",
        "graphql": "^15.0.0",
        "graphql-iso-date": "^3.6.1",
        "graphql-relay": "^0.6.0",
        "graphql-scalars": "^1.9.0",
        "graphql-subscriptions": "^1.2.1",
        "graphql-upload": "^11.0.0",
        "jsonwebtoken": "^8.5.1",
        "lodash": "^4.17.21",
        "moment": "^2.29.1",
